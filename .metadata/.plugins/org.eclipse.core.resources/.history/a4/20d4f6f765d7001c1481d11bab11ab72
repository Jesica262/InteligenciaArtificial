package frsf.cidisi.exercise.plantsVsZombies.search;

public class Zombie {

	private int id;
	private int tipo;
	private int ciclo;
	
	public Zombie(int id, int tipo, int ciclo) {
		super();
		this.id = id;
		this.tipo = tipo;
		this.ciclo = ciclo;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getTipo() {
		return tipo;
	}

	public void setTipo(int tipo) {
		this.tipo = tipo;
	}

	public int getCiclo() {
		return ciclo;
	}

	public void setCiclo(int ciclo) {
		this.ciclo = ciclo;
	}
}
